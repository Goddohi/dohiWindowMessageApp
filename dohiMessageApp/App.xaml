<Application x:Class="WalkieDohi.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WalkieDohi"
             xmlns:util="clr-namespace:WalkieDohi.Util"
             
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <util:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <util:InverseBoolToVisibilityConverter x:Key="InverseBoolToVisibilityConverter"/>
        <BitmapImage x:Key="WalkieDohiLogo" UriSource="pack://application:,,,/Assets/WalkieDohi.png" />
        <SolidColorBrush x:Key="PrimaryColor" Color="#FFBFDBD6"/>
        <SolidColorBrush x:Key="SecondaryColor" Color="#FF70AEB4" />
    </Application.Resources>
</Application>
